<% include ./Partials/Header.ejs %>
  <div class="w-full min-h-screen bg-zinc-800 p-20 text-white">
    <div class="flex flex-wrap gap-10">
        <% user.posts.forEach(function(elem){ %>
            <div class="w-[20vw]">
                <div class="card w-full h-52 bg-sky-200 rounded-xl overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                </div>
                <h5 class="text-xl font-semibold mt-3"><%=elem.title%></h5>
            </div>
     <%   }) %>
    </div>
  </div>
<% include ./Partials/Footer.ejs %>